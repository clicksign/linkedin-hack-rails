<p id="notice"><%= notice %></p>

<h1>Listing Profiles</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>First name</th>
      <th>Last name</th>
      <th>Company</th>
      <th>Title</th>
      <th>Address full</th>
      <th>Address city</th>
      <th>Address country</th>
      <th>Image</th>
      <th>Linkedin</th>
      <th>Linkedin url</th>
      <th>Linkedin company</th>
      <th>Website</th>
      <th>Gender</th>
      <th>Company gender</th>
      <th>Source</th>
      <th>Vertical</th>
      <th>Email</th>
      <th>Campaing</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @profiles.each do |profile| %>
      <tr>
        <td><%= profile.name %></td>
        <td><%= profile.first_name %></td>
        <td><%= profile.last_name %></td>
        <td><%= profile.company %></td>
        <td><%= profile.title %></td>
        <td><%= profile.address_full %></td>
        <td><%= profile.address_city %></td>
        <td><%= profile.address_country %></td>
        <td><%= profile.image %></td>
        <td><%= profile.linkedin_id %></td>
        <td><%= profile.linkedin_url %></td>
        <td><%= profile.linkedin_company_id %></td>
        <td><%= profile.website %></td>
        <td><%= profile.gender %></td>
        <td><%= profile.company_gender %></td>
        <td><%= profile.source %></td>
        <td><%= profile.vertical %></td>
        <td><%= profile.email %></td>
        <td><%= profile.campaing_id %></td>
        <td><%= link_to 'Show', profile %></td>
        <td><%= link_to 'Edit', edit_profile_path(profile) %></td>
        <td><%= link_to 'Destroy', profile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Profile', new_profile_path %>
