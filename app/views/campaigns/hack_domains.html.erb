<pre>
{
  "_id": "linkedin-company-url-campaign-<%= @campaign.id %>",
  "startUrl": [
    <% @companies.each_with_index do |company_id, index| %>
      "https://www.linkedin.com/sales/accounts/insights?companyId=<%= company_id %>&filter=potentialLeads"<%= "," if index != @companies.size - 1 %>
    <% end %>
  ],
  "selectors": [
    {
      "parentSelectors": [
        "_root"
      ],
      "type": "SelectorLink",
      "multiple": false,
      "id": "company-id",
      "selector": "a.see-more-link",
      "delay": ""
    },
    {
      "parentSelectors": [
        "_root"
      ],
      "type": "SelectorLink",
      "multiple": false,
      "id": "url",
      "selector": "div.hasNav-v2 div div div div div dd > a",
      "delay": ""
    }
  ]
}
</pre>
<br />
<%= link_to 'Back', @campaign %>
